  "license": "ISC",
  "devDependencies": {
    "@google/clasp": "^1.6.3",
    "@types/google-apps-script": "0.0.29",
    "del": "^3.0.0",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-googleappsscript": "^1.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "gulp": "^3.9.1",
    "prettier": "^1.14.3"
  },
